@{
    ViewBag.Title = "Home";
    var appName = "PrintPics.co.za";
}

<div class="inner cover">
    <h1 class="cover-heading">Welcome to @appName</h1>
    <p class="lead">We, at @appName, strive to make printing your favorite memories, captured in stunning images, easy to print. You don't have to queue, just submit and you will be notified when your images are ready.</p>
</div>

@section scripts
{
    <script>
        $(function() {
            printpics.setActiveMenu("home");
        });
    </script>
}